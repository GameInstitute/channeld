{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
    {
        "name": "Launch test function",
        "type": "go",
        "request": "launch",
        "mode": "test",
        "program": "${workspaceFolder}",
        "args": [
            "-test.run",
            "MyTestFunction"
        ]
    },
    {
        "name": "Launch Package",
        "type": "go",
        "request": "launch",
        "mode": "auto",
        "program": "${workspaceFolder}"
    },
    {
        "name": "Launch file",
        "type": "go",
        "request": "launch",
        "mode": "debug",
        "program": "${file}"
    },
    {
        "name": "Launch chat server",
        "type": "go",
        "request": "launch",
        "mode": "debug",
        "program": "examples/chat-rooms/main.go",
        "args": ["-dev", "-loglevel=0", "-profile=cpu", "-ct=1", "-chs=${workspaceFolder}/config/channel_settings_hifi.json"]
    },
    {
        "name": "Launch tank server",
        "type": "go",
        "request": "launch",
        "mode": "debug",
        "cwd": "${workspaceFolder}",
        "program": "examples/unity-mirror-tanks/main.go",
        "args": ["-cfsm=config/client_authoratative_fsm.json", "-dev", "-loglevel=-1", "-profile=cpu", "-ct=0", "-scc=${workspaceFolder}/config/spatial_static_tanks_small.json"]
    },
    {
        "name": "Launch TPS server",
        "type": "go",
        "request": "launch",
        "mode": "debug",
        "cwd": "${workspaceFolder}",
        "program": "examples/channeld-ue-tps/main.go",
        "args": ["-cfsm=config/client_authoratative_fsm.json", "-dev", "-loglevel=-1", "-ct=0", "-mcb=13", "-scc=${workspaceFolder}/config/spatial_static_2x2.json"]
    },
    {
        "name": "Launch replay session",
        "type": "go",
        "request": "launch",
        "mode": "debug",
        "program": "examples/replay/main.go",
        "args": ["-dev", "-loglevel=0", "-ct=1", "-chs=${workspaceFolder}/config/channel_settings_hifi.json"]
    },
    ]
}