    {
        "name": "Launch test function",
        "type": "go",
        "request": "launch",
        "mode": "test",
        "program": "${workspaceFolder}",
        "args": [
            "-test.run",
            "MyTestFunction"
        ]
    },
    {
        "name": "Launch Package",
        "type": "go",
        "request": "launch",
        "mode": "auto",
        "program": "${workspaceFolder}"
    },
    {
        "name": "Launch file",
        "type": "go",
        "request": "launch",
        "mode": "debug",
        "program": "${file}"
    },
    {
        "name": "Launch chat server",
        "type": "go",
        "request": "launch",
        "mode": "debug",
        "program": "examples/chat-rooms/main.go"
    },
    {
        "name": "Launch KCP",
        "type": "go",
        "request": "launch",
        "mode": "debug",
        "program": "cmd/main.go",
        "args": ["-sn", "kcp", "-cn", "kcp"]
    }
    ]
